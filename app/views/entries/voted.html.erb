<% @page_title = "投票したブログ一覧" %>
<%= @page_title %>

<% if @entries.present? %>
  <ul>
    <% @entries.each do |entry| %>
    <li>
      <%= link_to(entry.title, entry)%>
      by<%= link_to(entry.author.name, [entry.author, :entries])%>
      -<%= link_to("削除する", [:unlike, entry], method: :patch, data: { confirm: "本当に削除しますか？" })%>
    </li>
    <% end %>
  </ul>
<% else %>
  <p>投票した投稿はありません</p>
<% end %>
